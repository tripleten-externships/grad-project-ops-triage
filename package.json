{
  "name": "ops-triage-grad-project",
  "version": "1.0.0",
  "private": true,
  "description": "Multi-discipline bootcamp graduation project template for Ops Triage",
  "packageManager": "pnpm@10.28.1",
  "workspaces": [
    "packages/*",
    "contracts"
  ],
  "scripts": {
    "setup": "./scripts/setup.sh",
    "validate:contracts": "./scripts/validate-contracts.sh",
    "seed:data": "./scripts/seed-data.sh",
    "build": "pnpm -r --filter='./packages/*' build",
    "test": "pnpm -r --filter='./packages/*' test",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "check:deliverables": "./scripts/check-deliverables.sh",
    "dev": "pnpm -r --parallel --filter='./packages/*' dev"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "ajv": "^8.12.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "bootcamp",
    "monorepo",
    "graduation-project",
    "ops-triage"
  ],
  "author": "",
  "license": "MIT"
}
